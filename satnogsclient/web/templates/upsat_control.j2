                        <option value="14" >ADCS_GPS</option>
                        <option value="15" >ADCS_GPS</option>
                      </select>
                    </div>
                  </div>
                  <div class="row">
                    <div class="col-md-4">
                      <label>Type:</label>
                    </div>
                    <div class="col-md-8">
                      <select id="service-param-function" class="form-control">
                        <option selected style="display:none;">Type</option>
                        <option value="1" >ON</option>
                        <option value="0" >OFF</option>
                      </select>
                    </div>
                  </div>
                  <div class="row">
                    <div class="col-md-4">
                      <label>ACK:</label>
                    </div>
                    <div class="col-md-8">
                      <select id="service-param-power-ack" class="form-control">
                        <option selected style="display:none;">Select ACK</option>
                        <option value="0" >NO ACK</option>
                        <option value="1" >ACK</option>
                      </select>
                    </div>
                  </div>
                  <div class="row">
                    <div class="col-md-4">
                      <label>Destination:</label>
                    </div>
                    <div class="col-md-8">
                      <select id="service-param-power-dest_id" class="form-control">
                        <option selected style="display:none;">Dest ID</option>
                        <option value="6" >GND</option>
                        <option value="7" >UMB</option>
                      </select>
                    </div>
                  </div>
                  <div class="row">
                    <div class="col-md-4">
                    </div>
                    <div class="col-md-8">
                      <div class="text-right">
                        <button class="btn btn-primary"> Send </button>
                      </div>
                    </div>
                  </div>
                </div>
                <div id="service-param-time" style="display:none;">
                  <div class="row">
                    <div class="col-md-4">
                      <label>Function:</label>
                    </div>
                    <div class="col-md-8">
                      <select id="service-param-time-report" class="form-control">
                        <option selected style="display:none;">Function</option>
                        <option value="manual" >Set time in UTC</option>
                        <option value="auto" >Set current time in UTC</option>
                        <option value="utc" >Get UTC time</option>
                        <option value="qb50" >Get QB50 time</option>
                      </select>
                    </div>
                  </div>
                  <div class="row">
                    <div class="col-md-4">
                      <label>Destination:</label>
                    </div>
                    <div class="col-md-8">
                      <select id="service-param-time-dest_id" class="form-control">
                        <option selected style="display:none;">Dest ID</option>
                        <option value="6" >GND</option>
                        <option value="7" >UMB</option>
                      </select>
                    </div>
                  </div>
                  <div class="row">
                    <div class="col-md-4">
                      <label>ACK:</label>
                    </div>
                    <div class="col-md-8">
                      <select id="service-param-time-ack" class="form-control">
                        <option selected style="display:none;">Select ACK</option>
                        <option value="0" >NO ACK</option>
                        <option value="1" >ACK</option>
                      </select>
                    </div>
                  </div>
                  <div class="row">
                    <div class="col-md-4">
                      <label>Application:</label>
                    </div>
                    <div class="col-md-8">
                      <select id="service-param-time-app_id" class="form-control">
                        <option selected style="display:none;">Application ID</option>
                        <option value="1" >OBC</option>
                        <option value="3" >ADCS</option>
                      </select>
                    </div>
                  </div>
                  <div class="row" id="datetimepicker-time-row">
                    <div class="col-md-4">
                      <label>Time:</label>
                    </div>
                    <div class="col-md-8">
                      <div class='input-group date' id='datetimepicker-time' style="display:table;">
                        <input type='text' class="form-control" />
                        <span class="input-group-addon">
                            <span class="glyphicon glyphicon-calendar"></span>
                        </span>
                      </div>
                    </div>
                  </div>
                  <div class="row">
                    <div class="col-md-4">
                    </div>
                    <div class="col-md-8">
                      <div class="text-right">
                        <button class="btn btn-primary"> Send </button>
                      </div>
                    </div>
                  </div>
                </div>
              </div>
            </div>
          </div>
        </div>
      </div>
      <div class="col-md-6">
        <div class="panel panel-primary" id="response-panel">
          <div class="panel-heading">Terminal</div>
          <div class="panel-body" id="response-panel-body">
            <ul class="list-unstyled" id="log-list">
            </ul>
          </div>
          <div class="panel-footer">
            <div class="row">
              <div class="col-md-6">
                <div class="btn-group" role="group" aria-label="...">
                  <button type="button" class="btn btn-default" id="save-log">
                    <span class="glyphicon glyphicon-save-file" aria-hidden="true"></span>
                  </button>
                  <button type="button" class="btn btn-default" id="clear-log">
                    <span class="glyphicon glyphicon-remove" aria-hidden="true"></span>
                  </button>

                  <div class="btn-group" role="group">
                    <button type="button" class="btn btn-default dropdown-toggle" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                      <span class="glyphicon glyphicon-filter" aria-hidden="true"></span>
                      <span class="caret"></span>
                    </button>
                    <ul class="dropdown-menu" id="filter-section">
                      <li>
                        <input checked="true" type="checkbox" value="cmd"/>
                        <label>Command</label>
                      </li>
                      <li>
                        <input checked="true" type="checkbox" value="other"/>
                        <label>Other</label>
                      </li>
                    </ul>
                  </div>
                </div>
              </div>
              <div class="col-md-6">
                <div id="backend_online"></div>
              </div>
            </div>
          </div>
        </div>
        <div class="panel panel-primary" id="response-panel-wod">
          <div class="panel-heading">WOD</div>
          <div class="panel-body" id="response-panel-body-wod">
          </div>
        </div>
      </div>
    </div>
    <div class="row" id="network_mode">
    </div>
  </div>

{% endblock %}
{% block js %}
<script src="{{ url_for('static', filename='js/FileSaver.js') }}"></script>
<script src="{{ url_for('static', filename='js/upsat-control.js') }}"></script>
{% endblock %}
